{
  "name": "ngx-extended-async-pipe",
  "version": "1.0.0",
  "description": "Extended version of Angular's async pipe with customizable default, initial and error behavior",
  "homepage": "https://github.com/dscheerens/ngx-extended-async-pipe",
  "author": "Daan Scheerens",
  "license": "MIT",
  "keywords": [
    "angular",
    "ngx",
    "extended",
    "async",
    "asynchronous",
    "pipe",
    "observable",
    "promise"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dscheerens/ngx-extended-async-pipe"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project=demo",
    "serve.prod": "ng serve --project=demo --configuration production",
    "lint": "tslint -p lib/tsconfig.lib.base.json -t stylish -c lib/tslint.json",
    "test": "ng test --project=lib --watch=false --code-coverage",
    "test.watch": "ng test --project=lib --watch=true --code-coverage --browsers=Chrome",
    "build": "ng build --project=lib --configuration production",
    "build.demo": "ng build --project=demo --configuration production",
    "release": "standard-version --message \"release: v%s\"",
    "publish.dist": "npm publish build/dist"
  },
  "peerDependencies": {
    "@angular/core": ">=13.0.0",
    "rxjs": "^6 || ^7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.2.3",
    "@angular/animations": "^13.2.2",
    "@angular/cli": "^13.2.2",
    "@angular/common": "^13.2.2",
    "@angular/compiler": "^13.2.2",
    "@angular/compiler-cli": "^13.2.2",
    "@angular/core": "^13.2.2",
    "@angular/platform-browser": "^13.2.2",
    "@angular/platform-browser-dynamic": "^13.2.2",
    "@dscheerens/tslint-presets": "^9.0.0",
    "@types/jasmine": "~3.10.3",
    "codelyzer": "^6.0.2",
    "jasmine-core": "^3.10.1",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.2.1",
    "rxjs": "^6.6.7",
    "standard-version": "^9.3.2",
    "tslib": "^2.3.1",
    "tslint": "~6.1.3",
    "typescript": "~4.5.5",
    "typescript-tslint-plugin": "^1.0.1",
    "zone.js": "~0.11.4"
  }
}
