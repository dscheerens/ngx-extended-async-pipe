{
  "name": "ngx-extended-async-pipe",
  "version": "1.0.1",
  "description": "Extended version of Angular's async pipe with customizable default, initial and error behavior",
  "homepage": "https://github.com/dscheerens/ngx-extended-async-pipe",
  "author": "Daan Scheerens",
  "license": "MIT",
  "keywords": [
    "angular",
    "ngx",
    "extended",
    "async",
    "asynchronous",
    "pipe",
    "observable",
    "promise"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dscheerens/ngx-extended-async-pipe"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project=demo",
    "serve.prod": "ng serve --project=demo --configuration production",
    "lint": "ng lint --project=lib",
    "test": "ng test --project=lib --watch=false --code-coverage",
    "test.watch": "ng test --project=lib --watch=true --code-coverage --browsers=Chrome",
